pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo "build success"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "test success"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "deploy success"'
            }
        }
        stage('run') {
            steps {
                sh 'echo "run success"'
            }
    }
    } 
}
